<% include header%>
<% books.forEach(function(book){ %>
	<table class="table table-hover">
	  <% books.forEach(function(book,index){ %>
        <tr>
          <th>书名</th>
        	<td><%= book.title %></td>
        </tr>
        <tr>
        	<th>作者</th>
        	<td><%= book.author %></td>
        </tr>
        <tr>
          <th>出版社</th>
       		<td><%= book.publisher %></td>
        </tr>
        <tr>
        	<th>链接</th>
       		<td><%= book.link %></td>
        </tr>
        <tr>
        	<th>图片</th>
        	<td><%= book.img %></td>
        </tr>
        <tr>
          <th>状态</th>
        	<td><%= book.status %></td>
        </tr>
			<% }) %>
	</table>
<% }) %>
<% include footer%>